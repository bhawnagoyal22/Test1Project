import org.testng.annotations.Test;

public class seleniumgridTest {
	
	@Test
	public void login() {
	DesiredCapabilities cap= DesiredCapabilities.firefox();
	cap.setPlatform(Platform.WINDOWS);
	cap.setBrowserName("firefox");
	
	}

}
